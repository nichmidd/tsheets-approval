<table>
    <tr>
        <td>
            <img src="https://tsheets.mitservices.com.au/img/MITSG_Logo.jpg" width="140" style="width:140px;" />
        </td>
        <td>
            <table style="margin-left:20px;">
                <tr><td id="Ending" style="font-family:Arial, Helvetica, sans-serif; font-size: 13px; text-align:right; font-weight:700;">Week Ending: {{Cdate}} <td /></tr>
                <tr><td id="Approval" style="font-family:Arial, Helvetica, sans-serif; font-size:13px; text-align:right; font-weight:700;">Approved <td /></tr>
            </table>
        </td>
    </tr>
</table>
    <table class="Timesheet" cellspacing="0" style="text-align:center;margin-top:auto;margin-bottom:auto;margin-right:auto;margin-left:auto;border-style:solid;border-color:rgb(52, 52, 56);border-width:2px;background-color:rgb(52, 52, 56);">
        <tr>
            <th style="padding-top:10px;padding-bottom:10px;padding-right:10px;padding-left:10px;background-color:rgb(52, 52, 56);color:white;margin-top:0px;margin-bottom:0px;margin-right:0px;margin-left:0px;text-align:center;">Day</th>
            <th style="padding-top:10px;padding-bottom:10px;padding-right:10px;padding-left:10px;background-color:rgb(52, 52, 56);color:white;margin-top:0px;margin-bottom:0px;margin-right:0px;margin-left:0px;text-align:center;">Start</th>
            <th style="padding-top:10px;padding-bottom:10px;padding-right:10px;padding-left:10px;background-color:rgb(52, 52, 56);color:white;margin-top:0px;margin-bottom:0px;margin-right:0px;margin-left:0px;text-align:center;">End</th>
            <th style="padding-top:10px;padding-bottom:10px;padding-right:10px;padding-left:10px;background-color:rgb(52, 52, 56);color:white;margin-top:0px;margin-bottom:0px;margin-right:0px;margin-left:0px;text-align:center;">Contractor</th>
            <th style="padding-top:10px;padding-bottom:10px;padding-right:10px;padding-left:10px;background-color:rgb(52, 52, 56);color:white;margin-top:0px;margin-bottom:0px;margin-right:0px;margin-left:0px;text-align:center;">Hours</th>
        </tr>
        {{#each timesheets}}
        <tr>
            <td class="JSday" style="padding-right:10px;margin-top:0px;margin-bottom:0px;margin-right:0px;margin-left:0px;border-style:solid;border-width:2px;border-color:rgb(52, 52, 56);text-align:center;min-width:100px;min-height:30px;background-color:white;">{{this.day}}</td>
            <td class="JSst" style="padding-right:10px;margin-top:0px;margin-bottom:0px;margin-right:0px;margin-left:0px;border-style:solid;border-width:2px;border-color:rgb(52, 52, 56);text-align:center;min-width:100px;min-height:30px;background-color:white;">{{this.start}}</td>
            <td class="JSet" style="padding-right:10px;margin-top:0px;margin-bottom:0px;margin-right:0px;margin-left:0px;border-style:solid;border-width:2px;border-color:rgb(52, 52, 56);text-align:center;min-width:100px;min-height:30px;background-color:white;">{{this.end}}</td>
            <td style="padding-right:10px;margin-top:0px;margin-bottom:0px;margin-right:0px;margin-left:0px;border-style:solid;border-width:2px;border-color:rgb(52, 52, 56);text-align:center;min-width:100px;min-height:30px;background-color:white;">{{this.fn}} {{this.ln}}</td>
            <td style="padding-right:10px;margin-top:0px;margin-bottom:0px;margin-right:0px;margin-left:0px;border-style:solid;border-width:2px;border-color:rgb(52, 52, 56);text-align:center;min-width:100px;min-height:30px;background-color:white;">{{this.duration}}</td>
        </tr>
        {{/each}}
    </table>
<br />
<table>
    <tr>
        <td style="width: 330px;  text-align: center">
            <h3 id="total" style="font-size:18px;">Total: {{total}}</h3>
        </td>
    </tr>
</table>
<br />
<br />
